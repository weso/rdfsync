    <rdfs:label xml:lang="en">awards</rdfs:label>
    <owl:inverseOf rdf:resource="http://www.purl.org/hercules/asio/core#isAwardedBy"/>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:comment xml:lang="en">An object property linking an agent to something the agent awards, for example linking a funding agency to a grant, a university to a degree, or an organization to a prize. [source: cerif/frapo]</rdfs:comment>
