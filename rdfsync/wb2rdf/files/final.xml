    <rdfs:label xml:lang="en">capital</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.purl.org/hercules/asio/core#awards">
